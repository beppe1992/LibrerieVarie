<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" name="caching">

	<diskStore path="cache" />

	<cache name="METODO_CACHED" statistics="true" overflowToDisk="false"
		maxElementsInMemory="1000" eternal="false" timeToLiveSeconds="86400"
		diskPersistent="false" />

</ehcache>